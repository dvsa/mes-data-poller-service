  "author": "DVSA",
  "license": "MIT",
  "description": "All the code for moving data between TARS the caches in the MES estate",
  "homepage": "https://github.com/dvsa/mes-data-replication-service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvsa/mes-data-replication-service.git"
  },
  "dependencies": {
    "@types/luxon": "^1.4.0",
    "aws-sdk": "^2.389.0",
    "escape-json-node": "^2.0.0",
    "lodash": "^4.17.11",
    "luxon": "^1.4.5",
    "moment": "^2.23.0",
    "mysql": "^2.16.0",
    "winston": "^3.1.0",
    "node.extend": "^1.1.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.13",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.1.7",
    "@types/cucumber": "^4.0.5",
    "@types/jasmine": "^2.8.9",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.6",
    "@types/supertest": "^2.0.7",
    "aws-lambda-test-utils": "^1.3.0",
    "aws-sdk-mock": "^4.3.0",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "docker-compose": "^0.12.0",
    "husky": "^1.3.1",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "json-schema-to-typescript": "^6.1.0",
    "mysql-import": "^1.0.13",
    "node-docker-monitor": "^1.0.11",
    "node-fetch": "^2.3.0",
    "rimraf": "^2.6.2",
    "serverless": "^1.36.3",
    "serverless-dotenv-plugin": "^2.0.1",
    "serverless-dynamodb-local": "^0.2.35",
    "serverless-offline": "^4.2.0",
    "serverless-webpack": "^5.2.0",
    "supertest": "^3.4.2",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.1.3",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "yamljs": "^0.3.0"
  },
  "scripts": {
    "clean": "rimraf build artefacts",
    "compile": "tsc",
    "e2e": "cucumber-js e2e/ --require build/e2e/**/*.js",
    "bootstrap": "json2ts schema/exampleSchema.json > src/common/domain/Schema.d.ts",
    "lint": "tslint --project tslint.json",
    "lint:fix": "tslint --fix -p .",
    "package": "./package.sh",
    "postci": "npm run bootstrap",
    "postinstall": "npm run bootstrap",
    "prepush": "npm run security-scan && npm run lint && npm test",
    "security-scan": "git secrets --scan && git log -p | scanrepo",
    "start": "sls dynamodb install && sls offline start",
    "test": "npm run compile && jasmine",
    "test:integration": "npm run compile && jasmine build/spec/example.spec.js",
    "test:repos": "npm run compile && jasmine build/src/functions/pollJournals/framework/repo/mysql/__tests__/repositories.int.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run security-scan && npm run lint && npm test && npm run test:integration"
    }
  },
  "bugs": {
    "url": "https://github.com/dvsa/mes-data-replication-service/issues"
  }
}
