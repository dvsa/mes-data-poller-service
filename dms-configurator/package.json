{
    "name": "dms-configurator",
    "version": "0.0.1",
    "description": "DVSA Mobile Examiner Services - Generates and runs DMS tasks",
    "dependencies": {
        "aws-sdk": "^2.341.0",
        "escape-json-node": "^2.0.0",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@types/jest": "^23.3.7",
        "@types/node": "^8.10.36",
        "jest": "^23.6.0",
        "tslint": "^5.11.0",
        "tslint-config-airbnb": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "ts-jest": "^23.10.4",
        "typescript": "^3.1.3"
    },
    "scripts": {
        "test": "jest --env=node --colors --coverage",
        "start": "node build/cli-app.js",
        "clean": "rm -rf ./build",
        "compile": "tsc && npm run lint",
        "lint": "tslint -p . -c tslint.json --force"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dvsa/mes-data-replication-service"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "testMatch": [
            "**/*.spec.ts"
        ]
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://github.com/dvsa/mes-data-replication-service#readme"
